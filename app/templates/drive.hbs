{{#paper-content class="md-whiteframe-z1 list-demo"}}
  {{#paper-list}}
    {{#each model as |document|}}
      {{#paper-item class="md-3-line"}}
        <div class="md-list-item-text">
          <h3>{{document.title}}</h3>
          <h4>Modified by: {{document.lastModifyingUser.displayName}}</h4>
          <img src="{{document.iconLink}}" />
        </div>
      {{/paper-item}}
    {{/each}}
  {{/paper-list}}
{{/paper-content}}
